{
  "name": "ReactNativeWebviewQuillExampleApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native run-android",
    "test": "jest",
    "install": "npm run link-components",
    "link-components": "npm link react-native-webview-quill && metro-with-symlinks",
    "tsc": "tsc"
  },
  "dependencies": {
    "react": "16.6.3",
    "react-native": "0.58.5",
    "react-native-webview": "^5.2.1",
    "react-native-webview-quill": "github:luudjanssen/react-native-webview-quill"
  },
  "devDependencies": {
    "@types/jest": "^24.0.6",
    "@types/react": "^16.8.4",
    "@types/react-native": "^0.57.38",
    "@types/react-test-renderer": "^16.8.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "24.1.0",
    "jest": "24.1.0",
    "metro-react-native-babel-preset": "0.52.0",
    "metro-with-symlinks": "^1.3.1",
    "prettier": "^1.16.4",
    "react-test-renderer": "16.6.3",
    "ts-jest": "^24.0.0",
    "tslint": "^5.12.1",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.3.3333"
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "modulePaths": [
      "<rootDir>"
    ]
  }
}
